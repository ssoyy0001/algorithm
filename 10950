package algorithm;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		//두 정수 A와 B를 입력받은 다음, A+B를 출력하는 프로그램을 작성하시오.
		//첫째 줄에는 테스트케이스의 개수 T가 주어진다.
		//각 테스트케이스는 한 줄로 이루어져 있으며, 각 줄에 A와 B가 주어진다.  (0 < A, B < 10)
		//각 테스트케이스마다 A+B를 출력한다.
		
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();	//테스트개수
		int A;
		int B;
		int result[] = new int[T];
		
		for(int i=0; i<T; i++) {
			A = sc.nextInt();
			B = sc.nextInt();
			result[i] = A + B;
		}
		
		for(int r : result) {
			System.out.println(r);
		}
		
		sc.close();

	}
}
